<?xml version="1.0" encoding="UTF-8"?>
<config_plugin>

    <config>
       <workflow>Heris-Import</workflow>
       <template>HERIS</template>

	   <publicationType field="Typ" />

	   <defaultPublicationType>Document</defaultPublicationType>
       
       <metadata field="HERIS-ID" name="HerisID" />	   
       <metadata field="Alte Objekt-ID" name="DMDBID" />
       <metadata field="Gehört zu alter Objekt-ID" name="ParentElement" />
       <metadata field="Katalogtitel" name="TitleDocMain" />
       <metadata field="Typ" name="HerisType" />
       <metadata field="Hauptkategorie grob" name="MainCategory1" />
       <metadata field="Hauptkategorie mittel" name="MainCategory2" />
       <metadata field="Hauptkategorie fein" name="MainCategory3" />
       <metadata field="Gemeinden politisch (lt. Katastralgemeinden)" name="PoliticalCommunity" />
       <metadata field="Katastralgemeinde" name="CadastralCommune" />
       <metadata field="Bezirk" name="PoliticalDistrict" />
       <metadata field="Bundesland" name="FederalState" />
       <metadata field="Grundstücksnummern" name="PropertyNumber" />
       <metadata field="Bauzeit von" name="ConstructionTimeFrom" />
       <metadata field="Bauzeit bis" name="ConstructionTimeTo" />
       <metadata field="Publiziert" name="Published" />
       <metadata field="Straße" name="Street" />
       <metadata field="Hausnummer" name="StreetNumber" />
       <metadata field="PLZ" name="ZIPCode" />
       <metadata field="Zusatztext aus Adresse" name="AdditionalAddressText" />
       <metadata field="Weitere Adressen" name="OtherAddress" />
       <metadata field="Gehört zu HERIS-ID" name="ParentElement" />
       <metadata field="Ort" name="Community" />
       <metadata field="Staat" name="Country" /> 
    </config>
      

</config_plugin>